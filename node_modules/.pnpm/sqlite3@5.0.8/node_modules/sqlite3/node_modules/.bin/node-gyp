#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/node-gyp@8.4.1/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/sqlite3@5.0.8/node_modules/sqlite3/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/sqlite3@5.0.8/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
else 
  exec node  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
fi
